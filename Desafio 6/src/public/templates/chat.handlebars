{{#if chats}}
    <table class="table table-dark container mt-5">
        <tbody>
            {{#each chats}}
                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.message}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
<p>No hay Mensajes</p>
{{/if}}