{{#if products}}
    <table class="table">
        <thead class="thead-dark">
            <tr>
            <th scope="col">ID</th>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Picture</th>
        </tr>
        </thead>
    <tbody>
    {{#each products}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><img src="{{this.thumbnail}}" width="160px"></td>
    </tr>
    {{/each}}
    </tbody>
    </table>
{{else}}
<p>De momento no hay productos</p>
{{/if}}